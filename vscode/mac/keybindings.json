// Place your key bindings in this file to override the defaults
[
  {
      "key": "shift+cmd+;",
      "command": "editor.action.insertSnippet",
      "args": {
          "snippet": "console.debug(${TM_SELECTED_TEXT}$0);"
      },
      "when": "editorTextFocus"
  },
  {
      "key": "cmd+ctrl+up",
      "command": "moveActiveEditor",
      "args": {
          "to": "left",
          "by": "tab",
          "value": 1
      }
  },
  {
    "key": "cmd+ctrl+down",
    "command": "moveActiveEditor",
    "args": {
        "to": "right",
        "by": "tab",
        "value": 1
    }
  }
]
